<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
    <persistence-unit name="MCRG_PU" transaction-type="RESOURCE_LOCAL">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <exclude-unlisted-classes>false</exclude-unlisted-classes>
        <jta-data-source>java:app/jdbc/changesXML</jta-data-source>
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:postgresql://172.28.35.250:5432/mclawpkginfo"/>
            <property name="javax.persistence.jdbc.user" value="mclawpkginfo"/>
            <property name="javax.persistence.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="javax.persistence.jdbc.password" value="mclawpkginfo"/>
            <property name="eclipselink.logging.level" value="FINE"/>
            <property name="eclipselink.query-results-cache" value="false"/>
            <property name="eclipselink.refresh" value="true"/>
        </properties>
    </persistence-unit>
</persistence>
